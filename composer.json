{
    "name": "catlabinteractive/events",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "ext-json": "*",
        "ext-simplexml": "*",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-dompdf": "^2.0.0",
        "barryvdh/laravel-ide-helper": "^2.3",
        "catlabinteractive/central-storage-client": "~1.0.3",
        "catlabinteractive/charon-laravel": "~1.5.8",
        "catlabinteractive/eukles-client": "~1.0.8",
        "catlabinteractive/laravel-catlab-accounts": "~4.0",
        "catlabinteractive/laravel-charon-frontend": "~1.5.0",
        "catlabinteractive/laravel-table": "^1.0.2",
        "doctrine/dbal": "~2.5",
        "fab1en/rocket-chat-rest-client": "dev-master",
        "guzzlehttp/oauth-subscriber": "^0.6.0",
        "illuminate/console": "*",
        "laravel/framework": "~9.0.0",
        "laravelcollective/html": "^6.3",
        "paynl/sdk": "^1.5",
        "spatie/laravel-referer": "^1.8"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~9.5"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
